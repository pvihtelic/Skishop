<%= form_tag ski_url(@ski.id), :method => 'put' do %>
  	<%= label_tag :urk, "URL for Ski Image:" %>
	<%= text_field_tag :url %>

	<%= label_tag :title, "Title:" %>
	<%= text_field_tag :title %>

	<%= label_tag :price, "Price:" %>
	<%= text_field_tag :price %>

	<%= label_tag :link, "Link to Skis:" %>
	<%= text_field_tag :link %>

	<%= submit_tag "Edit Skis" %>
	<%= link_to "Go to Index", skis_url %>
<% end %>